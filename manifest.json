{

    "manifest_version": 3,
    "name": "CubeProfileScanner",
    "version": "1.1.0",
  
    "description": "Scan your CubeCraft forums profile and get the data in json",
  
    "icons": {
      "48": "icons/large.png"
    },

    "action": {
        "default_icon": "icons/small.png",
        "default_title": "CubeProfileScanner",
        "default_popup": "popup/popup.html"
    },

    "permissions": [
        "activeTab",
        "scripting"
    ]
}
  